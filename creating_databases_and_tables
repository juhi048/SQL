CREATE DATABASE temp1;
CREATE DATABASE temp2;

// to delete
DROP DATABASE temp1;
DROP DATABASE temp2;

DROP DATABASE IF EXISTS temp1;
DROP DATABASE IF EXISTS temp2; 

CREATE DATABASE college;
CREATE DATABASE IF NOT EXISTS college;

USE college;

CREATE TABLE student(
id INT PRIMARY KEY,
name VARCHAR(50),
age INT NOT NULL
);

// whole table can also be deleted
DROP TABLE student;

INSERT INTO student VALUES (1,"AMAN",26);
INSERT INTO student VALUES (2,"JUHI",27);

SELECT * FROM student;

SHOW DATABASES;
SHOW TABLES;
